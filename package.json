{
  "name": "loot",
  "author": "Oliver Hamlet <oliver.hamlet@gmail.com>",
  "version": "0.9.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/loot/loot.git"
  },
  "bugs": {
    "url": "https://github.com/loot/loot/issues"
  },
  "homepage": "https://github.com/loot/loot",
  "private": true,
  "dependencies": {
    "@octokit/rest": "^15.6.2",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-flex-layout": "^3.0.0",
    "@polymer/iron-icon": "^3.0.0",
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-iconset-svg": "^3.0.0",
    "@polymer/iron-list": "^3.0.0",
    "@polymer/iron-pages": "^3.0.0",
    "@polymer/neon-animation": "^3.0.0",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/paper-checkbox": "^3.0.0",
    "@polymer/paper-dialog": "^3.0.0",
    "@polymer/paper-dialog-scrollable": "^3.0.0",
    "@polymer/paper-dropdown-menu": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-input": "^3.0.0",
    "@polymer/paper-item": "^3.0.0",
    "@polymer/paper-listbox": "^3.0.0",
    "@polymer/paper-material": "^3.0.0",
    "@polymer/paper-menu-button": "^3.0.0",
    "@polymer/paper-progress": "^3.0.0",
    "@polymer/paper-ripple": "^3.0.0",
    "@polymer/paper-tabs": "^3.0.0",
    "@polymer/paper-toast": "^3.0.0",
    "@polymer/paper-toggle-button": "^3.0.0",
    "@polymer/paper-tooltip": "^3.0.0",
    "@polymer/polymer": "^3.0.5",
    "cytoscape": "^3.2.11",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-edgehandles": "^3.2.3",
    "decompress": "^4.0.0",
    "decompress-unzip": "^4.0.1",
    "fs-extra": "^5.0.0",
    "jed": "^1.1.1",
    "jed-gettext-parser": "^1.0.0",
    "lodash": "^4.17.5",
    "marked": "^0.3.17",
    "polymer-analyzer": "^2.7.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "replace": "^0.3.0",
    "request": "^2.79.0",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^22.4.2",
    "mkdirp": "^0.5.1",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1",
    "source-map-loader": "^0.2.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "scripts": {
    "build": "node ./scripts/get_roboto_files.js && webpack",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx ."
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-object-rest-spread"
    ],
    "presets": [
      "env",
      "react"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    },
    "globals": {
      "loot": false
    },
    "plugins": [
      "prettier"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
      "prettier"
    ],
    "rules": {
      "curly": [
        "error",
        "all"
      ],
      "linebreak-style": "off",
      "no-confusing-arrow": "error",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-underscore-dangle": "off",
      "prefer-destructuring": "off",
      "import/extensions": {
        "js": "always"
      },
      "prettier/prettier": "error",
      "react/destructuring-assignment": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/sort-comp": 0
    }
  },
  "jest": {
    "collectCoverage": true,
    "testRegex": "tests/gui/html/js/.+\\.js$"
  },
  "prettier": {
    "singleQuote": true
  }
}
